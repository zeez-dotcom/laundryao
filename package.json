{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "db:generate": "dotenv -e .env -- drizzle-kit generate",
    "db:generate:ci": "cross-env CI=1 dotenv -e .env -- drizzle-kit generate && echo 'Migrations generated successfully'",
    "db:migrate": "dotenv -e .env -- drizzle-kit migrate && echo 'Migrations applied successfully'",
    "db:push": "dotenv -e .env -- drizzle-kit push",
    "db:push:ci": "cross-env CI=1 dotenv -e .env -- drizzle-kit push && echo 'Schema pushed successfully'",
    "db:studio": "dotenv -e .env -- drizzle-kit studio",
    "db:migrate:ci": "cross-env CI=1 dotenv -e .env -- drizzle-kit migrate && echo 'Migrations applied successfully'",
    "db:prepare": "wait-on tcp:localhost:5432 && npm run db:migrate",
    "db:seed": "dotenv -e .env -- tsx server/db/seed.ts",
    "db:audit:uniques": "npm run db:psql scripts/sql/audit_branch_uniques.sql",
    "db:cleanup-tests": "npm run db:psql scripts/sql/cleanup_test_artifacts.sql",
    "db:dedupe": "dotenv -e .env -- tsx scripts/dedupe-branch-uniques.ts",
    "dev": "dotenv -e .env -- tsx -r dotenv/config server/index.ts",
    "start": "node dist/server/index.js",
    "build": "npm run build:server",
    "build:server": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json --resolve-full-paths --resolve-full-extension .js",
    "build:client": "vite build",
    "check": "tsc --noEmit",
    "lint:no-console": "node scripts/no-console-check.cjs",
    "test": "vitest run --pool=vmThreads && node --import tsx --test server/*.test.ts",
    "validate:srs": "node scripts/validate-srs.js",
    "db:psql": "psql \"$DATABASE_URL\" -f",
    "db:migrate:add-completed": "psql \"$DATABASE_URL\" -f migrations/0016_add_completed_status.sql",
    "db:migrate:fix-categories": "psql \"$DATABASE_URL\" -f migrations/0017_remove_category_duplicates.sql",
    "db:backfill-package-items": "dotenv -e .env -- tsx server/backfill-package-items.ts",
    "db:backfill-item-service-prices-branch": "dotenv -e .env -- tsx server/backfill-item-service-prices-branch.ts",
    "seed": "dotenv -e .env -- tsx scripts/seed.ts",
    "db:truncate": "npm run db:psql scripts/sql/truncate_all_user_tables.sql",
    "db:truncate:node": "dotenv -e .env -- tsx scripts/truncate-db.ts",
    "db:rls:enable": "dotenv -e .env -- tsx scripts/toggle-rls.ts enable",
    "db:rls:disable": "dotenv -e .env -- tsx scripts/toggle-rls.ts disable"
  },
